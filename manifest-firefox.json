{
  "manifest_version": 2,
  "name": "notionthemes",
  "version": "0.0.1",
  "description": "Beautiful themes for Notion Web.",
  "permissions": ["storage", "https://notionthemes.netlify.app/"],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.notion.so/*"],
      "js": ["js/content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Choose a theme",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "icons/16.png",
      "38": "icons/48.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true,
    "open_in_tab": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "42.0"
    }
  }
}
